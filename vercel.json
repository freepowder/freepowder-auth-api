
{
  "version": 2,
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
  "builds": [
    {
      "src": "src/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/index.js"
    }
  ]
}
